{
  "name": "vite-plugin-feature-flag",
  "version": "1.0.0-alpha.2",
  "description": "A Vite plugin for managing feature flags in your code",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "diligent-rookie",
  "license": "MIT",
  "engines": {
    "node": ">=20.8.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diligent-rookie/vite-plugin-feature-flag.git",
    "directory": "packages/feature-flag-plugin"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/diligent-rookie/vite-plugin-feature-flag/issues"
  },
  "scripts": {
    "dev": "unbuild --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "feature-flag",
    "feature-toggle"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "magic-string": "^0.30.17"
  },
  "peerDependencies": {
    "vite": ">=5.0.0",
    "rollup": ">=4.0.0"
  },
  "devDependencies": {
    "unbuild": "^3.6.0",
    "typescript": "^5.0.0"
  }
}
